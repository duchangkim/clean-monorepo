{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "library/react-example/data-access/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/library/react-example/data-access",
        "main": "library/react-example/data-access/src/index.ts",
        "tsConfig": "library/react-example/data-access/tsconfig.lib.json",
        "assets": ["library/react-example/data-access/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["library/react-example/data-access/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/library/react-example/data-access"],
      "options": {
        "jestConfig": "library/react-example/data-access/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:data-access"]
}
